<h1>Register</h1>

<form  method="post" action="/sessions/register">
<input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>"
<p>First Name : <input type="text" name="users[first_name]"  >   </p>

<p>Last Name : <input type="text" name="users[last_name]"  >   </p>

<p>Email : <input type="text" name="users[email]" ></p>
<p>Password : <input type="password" name="users[password]"></p>
<p>Confirm Password : <input type="password" name="users[password_confirmation]" ></p>
<input type="submit" value="Submit">
</form>


<h1>Login</h1>

<form method="post" action="/sessions/login">
<input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
<p>Email : <input type="text" name="user[email]"></p>
<p>Password : <input type="password" name="user[password]"></p>
<input type="submit" value="Submit">
</form>
